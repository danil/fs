/home/linuxbrew/.linuxbrew/var/log/tarantool/*.log {
    daily
    size 512k
    missingok
    rotate 10
    compress
    delaycompress
    create 0640 tarantool adm
    postrotate
        /home/linuxbrew/.linuxbrew/Cellar/tarantool/2.10.6/bin/tarantoolctl logrotate `basename ${1%%.*}`
    endscript
}
